<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RelativeActivityDeepak">


<!--    Linear Layout , orientation vertical, horizontal-->

    <!--   Overlapping -->

    <TextView
        android:id="@+id/tvRajat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginVertical="50dp"
        android:text="Rajat Guleria"
        android:layout_below="@+id/tvDeepak"
        android:textSize="20sp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Varsha"
        android:layout_marginHorizontal="20dp"
        android:layout_toEndOf="@+id/tvDeepak"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/tvDeepak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deepak Kumar"
        android:textSize="20sp" />




</RelativeLayout>